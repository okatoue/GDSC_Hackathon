{
    "builds": [{
        "src": "/stream/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "15mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "stream/wsgi.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
          }
    ],
    "rewrites": [{ "source": "/(.*)", "destination": "/index.html" }]
}